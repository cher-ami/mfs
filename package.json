{
  "name": "@wbe/mfs",
  "version": "0.1.1",
  "description": "nodejs fs wrapper",
  "author": "Willy Brauner",
  "type": "module",
  "license": "MIT",
  "source": "src/mfs.ts",
  "main": "dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "private": false,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/mfs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && microbundle",
    "build:watch": "microbundle watch --no-sourcemap --format esm",
    "test:watch": "vitest",
    "test": "vitest run",
    "pre-publish": "npm run clean && npm run build && npm run test"
  },
  "dependencies": {
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "microbundle": "^0.15.0",
    "typescript": "^4.7.4",
    "vitest": "^0.18.1"
  }
}
